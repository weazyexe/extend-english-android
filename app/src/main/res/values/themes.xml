<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorNearlyWhite</item>
        <item name="colorPrimaryDark">@color/colorNearlyWhite</item>
        <item name="colorAccent">@color/colorOrange</item>
        <item name="android:navigationBarColor">@color/colorNearlyWhite</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:textColor">@color/colorBlack</item>
